{
  "name": "jsreport-mongodb-store",
  "version": "1.3.1",
  "description": "jsreport extension adding support for storing report templates inside mongodb",
  "main": "index.js",
  "homepage": "https://github.com/jsreport/jsreport-mongodb-store",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-mongodb-store.git"
  },
  "scripts": {
    "test": "mocha --timeout 15000 test && standard",
    "test-with-transactions": "cross-env RUN_TRANSACTIONS_TESTS=true USE_REPLICA=true mocha --timeout 15000 test && standard"
  },
  "keywords": [
    "jsreport",
    "mongodb"
  ],
  "standard": {
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "dependencies": {
    "mongodb": "3.4.0"
  },
  "devDependencies": {
    "cross-env": "6.0.3",
    "eslint": "5.14.1",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "jsreport-core": "2.7.0",
    "mocha": "5.2.0",
    "should": "13.2.3",
    "standard": "12.0.1"
  },
  "author": "Jan Blaha",
  "engines": {
    "node": ">=8.9"
  }
}
